{
  "name": "eslint-config-hudl",
  "version": "8.0.0",
  "description": "Hudl's ESLint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hudl/javascript"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "airbnb",
    "javascript",
    "styleguide"
  ],
  "author": "Jake Teton-Landis (https://twitter.com/@jitl)",
  "contributors": [
    {
      "name": "Jake Teton-Landis",
      "url": "https://twitter.com/jitl"
    },
    {
      "name": "Jordan Harband",
      "email": "ljharb@gmail.com",
      "url": "http://ljharb.codes"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hudl/javascript/issues"
  },
  "homepage": "https://github.com/hudl/javascript/packages/eslint-config-hudl",
  "publishConfig": {
    "registry": "http://npm-push.hudltools.com"
  },
  "hudl-ci": {
    "buildCommand": null,
    "lintCommand": "npm run lint",
    "testCommand": "npm run test"
  },
  "tasksConfig": {
    "test": {
      "src": [
        "lib/**/*.js",
        "!**/__specs__/**/*.js"
      ],
      "tests": "**/*-specs.js",
      "setup": "./test/setup",
      "coverageThresholds": {
        "global": 100
      }
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-teamcity": "^1.0.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^4.0.0"
  },
  "peerDependencies": {
    "eslint": "^5.9.0 || ^6.8.0"
  }
}
